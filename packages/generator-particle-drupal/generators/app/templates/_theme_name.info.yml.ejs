name: <%= themeNamePascal %>
type: theme
description: <%= themeNameDescription %>
base theme: stable
core_version_requirement: ^8.8 || ^9
libraries:
  - <%= themeNameSnake %>/core
libraries-override:
  core/jquery: <%= themeNameDescription %>/jquery
regions:
  primary_menu: Primary menu
  highlighted: Highlighted
  header_branding: Header branding
  header_cta: Header CTA
  secondary_menu: Secondary menu
  page_top: Page top
  page_bottom: Page bottom
  breadcrumb: Breadcrumb
  content: Content
  sidebar_first: Sidebar first
  sidebar_second: Sidebar second
  footer: Footer
component-libraries:
  protons:
    paths: []
  atoms:
    paths:
      - <%= drupalDist %>/app-drupal/assets/atomic/_patterns/01-atoms
  molecules:
    paths:
      - <%= drupalDist %>/app-drupal/assets/atomic/_patterns/02-molecules
  organisms:
    paths:
      - <%= drupalDist %>/app-drupal/assets/atomic/_patterns/03-organisms
  templates:
    paths: []
  pages:
    paths: []
