# Optional Config for Responsive Image
<%= themeNameSnake %>.xs:
  label: xs
  mediaQuery: '(min-width: 0px)'
  weight: 0
  multipliers:
    - 1x
    - 2x
<%= themeNameSnake %>.sm:
  label: sm
  mediaQuery: '(min-width: 640px)'
  weight: 1
  multipliers:
    - 1x
    - 2x
<%= themeNameSnake %>.md:
  label: md
  mediaQuery: '(min-width: 768px)'
  weight: 2
  multipliers:
    - 1x
    - 2x
<%= themeNameSnake %>.lg:
  label: lg
  mediaQuery: '(min-width: 1024px)'
  weight: 3
  multipliers:
    - 1x
    - 2x
<%= themeNameSnake %>.xl:
  label: xl
  mediaQuery: '(min-width: 1280px)'
  weight: 4
  multipliers:
    - 1x
    - 2x
